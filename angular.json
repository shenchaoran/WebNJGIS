{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "CMIP-frontend": {
            "root": "",
            "sourceRoot": "src",
            "projectType": "application",
            "architect": {
                "build": {
                    "builder": "@angular-builders/custom-webpack:browser",
                    "options": {
                        "customWebpackConfig": {
                            "path": "./extra-webpack.config.js",
                            "mergeStrategies": {
                                "loaders": "replace"
                            },
                            "replaceDuplicatePlugins": true
                        },
                        "deleteOutputPath": true,
                        "outputPath": "build-prod",
                        "watch": true,
                        "vendorChunk": true,
                        "commonChunk": true,
                        "statsJson": true,
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "tsConfig": "src/tsconfig.app.json",
                        "polyfills": "src/polyfills.ts",
                        "assets": [
                            "src/assets",
                            "src/config",
                            "src/browser-download.html",
                            "src/favicon.ico"
                        ],
                        "styles": [
                            "src/styles.scss"
                        ],
                        "stylePreprocessorOptions": {
                            "includePaths": [
                                "node_modules"
                            ]
                        },
                        "scripts": [
                            "node_modules/lodash/lodash.min.js",
                            "node_modules/jquery/dist/jquery.min.js",
                            "src/assets/js/jexcel/js/jquery.jexcel.js",
                            "node_modules/material-components-web/dist/material-components-web.min.js"
                        ]
                    },
                    "configurations": {
                        "hmr": {
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.hmr.ts"
                            }]
                        },
                        "prod": {
                            "statsJson": false,
                            "outputPath": "build-prod",
                            "optimization": false,
                            "buildOptimizer": false,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.prod.ts"
                            }]
                        },
                        "prod-optimizer": {
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "outputPath": "build-prod-optimizer",
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.prod.ts"
                            }]
                        },
                        "aot": {
                            "browserTarget": "CMIP-frontend:build:prod",
                            "outputPath": "build-aot",
                            "aot": true,
                            "sourceMap": true,
                            "extractCss": true,
                            "namedChunks": true,
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.hmr.ts"
                            }]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-builders/dev-server:generic",
                    "options": {
                        "proxyConfig": "proxy.conf.json",
                        "open": false,
                        "liveReload": true,
                        "hmrWarning": false,
                        "baseHref": "/CMIP/",
                        "port": 8888,
                        "host": "localhost",
                        "hmr": true,
                        "browserTarget": "CMIP-frontend:build:hmr"
                    },
                    "configurations": {
                        "aot": {
                            "browserTarget":"CMIP-frontend:build",
                            "aot": true,
                            "optimization": true,
                            "open": true,
                            "hmr": false,
                            "sourceMap": true
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "CMIP-frontend:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/test.ts",
                        "karmaConfig": "./karma.conf.js",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "src/tsconfig.spec.json",
                        "scripts": [],
                        "styles": [],
                        "assets": []
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "src/tsconfig.app.json",
                            "src/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        },
        "CMIP-frontend-e2e": {
            "root": "",
            "sourceRoot": "e2e",
            "projectType": "application",
            "architect": {
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "./protractor.conf.js",
                        "devServerTarget": "CMIP-frontend:serve"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "e2e/tsconfig.e2e.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        }
    },
    "defaultProject": "CMIP-frontend",
    "schematics": {
        "@schematics/angular:class": {
            "spec": false
        },
        "@schematics/angular:component": {
            "spec": false,
            "index": true,
            "prefix": "ogms",
            "styleext": "scss"
        },
        "@schematics/angular:directive": {
            "flat": true,
            "spec": false,
            "prefix": "ogms"
        },
        "@schematics/angular:guard": {
            "flat": true,
            "spec": false
        },
        "@schematics/angular:interface": {
            "prefix": ""
        },
        "@schematics/angular:module": {
            "flat": false,
            "spec": false,
            "routing": true
        },
        "@schematics/angular:pipe": {
            "flat": true,
            "spec": false
        },
        "@schematics/angular:service": {
            "flat": true,
            "spec": false
        }
    }
}
